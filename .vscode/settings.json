{
  "search.exclude": {
    "package-lock.json": true
  },

  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": true,
    "source.fixAll": true
  },
  "eslint.validate": [
    "javascript",
    "javascriptreact",
    "typescript",
    "typescriptreact"
  ],
  "files.associations": {
    "*.json": "jsonc"
  },
  "typescript.preferences.importModuleSpecifier": "non-relative",
  "typescript.validate.enable": true
}
