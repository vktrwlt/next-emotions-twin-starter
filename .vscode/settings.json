{
  "search.exclude": {
    "package-lock.json": true
  },

  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": true,
    "source.fixAll": true
  },
  "eslint.validate": [
    "javascript",
    "javascriptreact",
    "typescript",
    "typescriptreact"
  ],

  "typescript.preferences.importModuleSpecifier": "non-relative",
  "typescript.validate.enable": true
}
